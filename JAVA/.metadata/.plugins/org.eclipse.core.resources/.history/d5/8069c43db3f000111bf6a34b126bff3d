
/**
 * 
 * @author W.Chen
 * 
 */
public final class HW23 {

	/**
	 * HW2_1 is a utility class.
	 */
	private HW23() {
	}

	/**
	 * @param args
	 *            command line arguments
	 */
	public static void main(String[] args) {
		int i = 0, j = 10, n = 0;
		while (i < j) {
			i++;
			j--;
			n++;
		}

		System.out.println("i= " + i);
		System.out.println("j= " + i);
		System.out.println("n= " + i);

	}

}
